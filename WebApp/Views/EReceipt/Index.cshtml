                                            </div>
                                        </div>
                                    </div>
                                    @if (Model.objConfig.IsUpdateProfileAndEarn)
                                    {
                                        <div id="divProfileUpdate" class="banner_bg" style="display:block;margin-top:20px;margin-bottom:20px;" background="">
                                            <img src="~/EReceiptBanners/UpdateProfileBGImage.png" id="imgProfileUpdate" style="margin-left:0px;" class=" banner_img_text banner_img_text1" alt="banner">
                                            <div class="bg_content_details">
                                                <h3>
                                                    <span id="lblProfileUpdateTitle">Update Profile and Earn reward</span>
                                                </h3>
                                                <h6>
                                                    <span id="lblProfileUpdateBodyText">Rs 100 off on min shopping of Rs 999 (valid on fresh articles)</span>
                                                </h6>
                                                <button id="btnProfileUpdate" class="update_btn"><a href="#" id="ancProfileUpdate" target="_blank">Update Your Profile</a></button>
                                            </div>
                                        </div>
                                    }
                                    @if (Model.objConfig.IsSocialMedia)
                                    {

                                        <div id="divSocial" class="social_card_wrap portlet" style="display: block; margin-top: 0px; margin-left: 0px !important; margin-right: 0px !important;">
                                            <div>
                                                <div class="editorTool"></div>
                                                <div class="social_content">
                                                    <div class="question_heading">
                                                        <h2>
                                                            <span id="lblSocialTitle">Stay connected</span>
                                                        </h2>
                                                    </div>
                                                    <div class="social_icon_wrap">
                                                        @if (!string.IsNullOrEmpty(Model.objConfig.FacebookUrl))
                                                        {
                                                            <div id="divFBURL" class="social_icon" style="display:block;">
                                                                <a id="HlnkFBURL" target="_blank" href="@Model.objConfig.FacebookUrl"><img src="~/Content/EReceipt/Images/facebook.svg" alt="facebook" style="margin-bottom:3px;"></a>
                                                            </div>
                                                        }
                                                        @if (!string.IsNullOrEmpty(Model.objConfig.TwitterUrl))
                                                        {
                                                            <div id="divtwitterURL" class="social_icon" style="display:block;">
                                                                <a id="HlnktwitterURL" target="_blank" href="@Model.objConfig.TwitterUrl"><img src="~/Content/EReceipt/Images/twitter.svg" alt="twitter" style="margin-bottom:3px;"></a>
                                                            </div>
                                                        }
                                                        @if (!string.IsNullOrEmpty(Model.objConfig.InstagramUrl))
                                                        {
                                                            <div id="divinstagramURL" class="social_icon" style="display:block;">
                                                                <a id="HlnkinstagramURL" target="_blank" href="@Model.objConfig.InstagramUrl"><img src="~/Content/EReceipt/Images/instagram.svg" alt="instagram" style="margin-bottom:3px;"></a>
                                                            </div>
                                                        }
                                                        @if (!string.IsNullOrEmpty(Model.objConfig.YoutubeURL))
                                                        {
                                                            <div id="divyoutubeURL" class="social_icon" style="display:block;">
                                                                <a id="HlnkyoutubeURL" target="_blank" href="@Model.objConfig.YoutubeURL"><img src="~/Content/EReceipt/Images/youtube.svg" alt="youtube" style="margin-bottom:3px;"></a>
                                                            </div>
                                                        }
                                                        @if (!string.IsNullOrEmpty(Model.objConfig.WhatsappUrl))
                                                        {
                                                            <div id="divwhatsappurl" class="social_icon" style="display:block;">
                                                                <a id="HlnkwhatsappURL" target="_blank" href="https://api.whatsapp.com/send/?phone=@Model.objConfig.WhatsappUrl&amp;text&amp;app_absent=0"> <img src="~/Content/EReceipt/Images/whatsapp.svg" width="19" height="20" alt="whatsapp" style="margin-bottom:4px;"></a>
                                                            </div>
                                                        }
                                                    </div>


                                                </div>
                                            </div>
                                        </div>
                                    }
                                    <div id="divUniversalTermsAndConditionMain" class="invoice_terms_wrap portlet" style="display: block; margin-left: 0px !important; margin-right: 0px !important;">
                                        <a id="ancUniversalTAndC" href="javascript:void(0);" onclick="fncUniversalTermsAndConditionSlide();">
                                            <label><span id="lblUniversalTermsAndCondition">Terms and Conditions </span><i class="fa fa-caret-down" aria-hidden="true"></i></label>
                                        </a>
                                        <div id="divUniversalTermsAndCondition" class="tc_wrap" style="display: none;">

                                            <ul id="UlUT1" style="display:block;">
                                                <span id="spnUniversalTermsAndCinditions">@Html.Raw(Model.objConfig.TermsAndConditions)</span>
                                            </ul>
                                            <ul id="UlUT2" style="display: none;">
                                                <li>
                                                    <span id="lblUniversalTermsAndCinditionsSecond"></span>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div id="divUniversalThankANDVisit" class="thank_you_content">
                                        <label id="lblUniversalThankyouMsg" style="display:block;text-align:center;margin-bottom:10px;margin-top:10px;">Thank you ! <br> Please visit again.</label>

                                        <ul id="ulThankYouStoreEmailAndMobile" style="display:;list-style:none;text-align:center;margin-bottom:0px;">
                                            <li id="liThankyouStoreEmail" style="display:;">
                                                <img src="~/Content/EReceipt/Images/mail-outline.png" id="imgThankyouStoreEmail" style="width:15px;display:;">&nbsp;<span id="lblThankYouStoreHelp">@Model.objConfig.CustomerServiceEmail</span>
                                            </li>
                                            <li id="liThankyouStoreMobile" style="display:;">
                                                <img src="~/Content/EReceipt/Images/phone-portrait-outline.png" id="imgThankYouStoreMobile" style="width:10px;display:;">&nbsp;<span id="lblThankYouStoreContact">@Model.objConfig.CustomerServiceContact</span><br>
                                            </li>
                                            <li>
                                            </li>
                                        </ul>
                                        <br>

                                    </div>
                                    <p id="pThankYouDisclaimer" class="disclaimer">This is system generated receipt</p>
                                    <p class="Powered_by">
                                        Powered by Blueocktopus<br>

                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
        function showmorefn() {
            document.getElementById("comp_address").style.display = "block";
            document.getElementById("show_more_content").style.display = "none";
            document.getElementById("show_less_content").style.display = "block";
        }
        function showlessfn() {
            document.getElementById("comp_address").style.display = "none";
            document.getElementById("show_more_content").style.display = "block";
            document.getElementById("show_less_content").style.display = "none";
        }

        function fncAmountslide(id, current) {
            var nextEle = $('#' + id).css('display');
            if (nextEle == 'block') {
                $('#' + id).slideDown();
                $('.totalPointBlock').hide();
            }
            else {
                $('.totalPointBlock').slideUp();
                $('.totalPointBlock').show();
            }
        }
        function showMoreLess() {
            let isVisible = true;
            //$("#ctl00_ContentPlaceHolder1_lbtnShowMore").click(function () {
            isVisible = !isVisible;
            if ($('#spnInvoice').text() == "Show Less") {
                $('#spnInvoice').text('Show More');
            }
            else {
                $('#spnInvoice').text('Show Less');
            }

            $(".show_more_toggle").slideToggle();
            $(".invoice_section_wrap").find('.edit_border_wrap').toggleClass('showless');
            //});
        }
        function fncUniversalTermsAndConditionSlide() {
            var nextEle = $('#' + "divUniversalTermsAndCondition").css('display');
            if (nextEle == 'block') {
                $('#' + "divUniversalTermsAndCondition").slideUp();
                $('#' + "divUniversalTermsAndCondition").hide(1000);
                $('#' + "divUniversalTermsAndCondition").css("display", "none");
            }
            else {
                $('#' + "divUniversalTermsAndCondition").slideDown();
                $('#' + "divUniversalTermsAndCondition").show(1000);
                $('#' + "divUniversalTermsAndCondition").css("display", "block");
            }
        }
    </script>
</body>

</html>
