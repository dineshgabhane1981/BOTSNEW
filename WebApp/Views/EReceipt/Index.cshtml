        }

        function fncPDFErrorMessageClose() {
            $("#divSendMailPDFErrorMessage").css('display', 'none');
            //$("#divSendMailPDFErrorMessage").hide();
        }</script>*@
</head>
<body>
    <div class="pre-loader-block" id="loading" style="display: none;">
        <div class="loader-inner">
            <div class="loader-text">
                <img src="./Content/EReceipt/SVG/preloader.svg" alt="" height="60" width="60">
            </div>
        </div>
    </div>

    <div class="bg-grey">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div id="divReceipt" class="template_section">
                        <div id="divHeader" class="invoice_header_section portlet" style="display:block;">
                            <div class="edit_border_wrap">
                                <!-- Edit Highlight Tool -->
                                <div class="invoice_heading">
                                    <h3>
                                        <span id="lblInvoiceHeading">E - RECEIPT</span>
                                    </h3>
                                </div>
                                <div class="invoice_heading">
                                    <div class="invoice_logo mb-2" style="margin-bottom: 23px !important;">
                                        <img id="imgHeaderImage" class="img-fluid" src="@Model.BrandLogo" width="100px" style="border-width:0px;">
                                    </div>
                                    <!--  New template change -->

                                    <h2 id="h2UniversalStoreName" class="StoreName">@Model.BrandName</h2>
                                    <!-- new template chnage -->
                                    <!-- Show more & Show less  dropdown Start -->
                                    <h6 id="show_more_content" onclick="showmorefn()" class="show-more-content">
                                        <u>Show More</u>
                                        <img src="./Content/EReceipt/SVG/showmore.svg" alt="Down arrow">
                                    </h6>
                                    <div id="comp_address" style="display: none">
                                        <div class="invoice_map">
                                            <p class="text-center">
                                                <span id="lblStoreDetails">@Model.StoreAddress, Tel:@Model.StoreContact</span>
                                            </p>
                                        </div>
                                        <div id="divUniversalTemplateStoreDetails" style="display:;">
                                            <ul id="ulStoreTiming" style="display: none;">
                                                <li id="liStoreTiming" style="display: none;">
                                                    <span id="lblStoreTiming"></span>
                                                </li>
                                                <li id="liStoreCutomercare" style="display:inline-block;">
                                                    <span id="lblStoreCutomercare">@Model.StoreContact</span>
                                                </li>
                                            </ul>
                                            <div id="divStoreCINAndPAN" class="d-store" style="display:flex;">
                                                <div class="store-details">
                                                    <div id="divStoreCIN" class="" style="display:block;">
                                                        @*<h4 id="h4StorePan"><span id="spnLabelStorePAN">Store PAN:</span><span id="lblStorePANMasterValue">AABCB1043Q</span></h4>
                                                            <h4 id="h4StoreCIN"><span id="spnLabelStoreCIN">Store CIN:</span> <span id="lblStoreCIN">L19201WB1931PLC007261</span></h4>*@
                                                        <p id="lblStoreGSTIN">GSTIN # @Model.objPOSBILL.OwnerGSTINNo</p>
                                                        @*<p id="pStoreCode" class="fass-reg"><span id="spnStoreCodeLabel">Store Code:</span> <span id="spnStoreCode">smb2499</span></p>*@
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <h6 id="show_less_content" onclick="showlessfn()" style="display: none" class="show-more-content">
                                        <u>Show Less</u>
                                        <img src="./Content/EReceipt/SVG/showmore.svg" class="rotate_img" alt="Down arrow">
                                    </h6>
                                    <!-- Show more & Show less  dropdown emd -->
                                    <span id="spnvisitusTitle">Visit us at </span><a href="https://@Model.WebsiteURL" target="_blank" id="ancvisitUSURL" style="text-decoration: none; font-size: 14px;" target="_blank">
                                        <span id="lblvisitUSURL">@Model.WebsiteURL</span>
                                    </a><br>
                                </div>
                            </div>
                        </div>
                        <div id="divUniversalAdressBlock">
                            <div id="divUniVersalTemplateHeaderBlock" class="col-12 addressBlock">
                                <p class="mb-0" style="text-align: center"></p>
                                <h2 class="mt-3" style="font-weight: 700 !important;">
                                    <span id="lblHeaderTitle2">Dear Member</span>
                                </h2>
                                <h2 class="m-0 d-block w-100">
                                    <a href="#" id="ancHederMemberContact" class="m-0 d-block w-100 position-static">
                                        <span id="lblHederMemberContact"><b>Mobile No:</b> @Model.objCustomer.HashedMobile</span>
                                    </a>
                                </h2>
                                <a href="#" id="ancEditProfileURL" style="font-weight: 400 !important;" target="_blank">
                                    <img src="./Content/EReceipt/images/edit.png" alt="Click to Edit your profile details" title="Click to Edit your profile details" style="width: 23px;">
                                    Edit Profile
                                </a>
                                <p class="m-0 d-block w-100">
                                    <a href="#" id="ancHeaderMemberEmail" class="m-0 d-block w-100 position-static">
                                        <span id="lblHeaderMemberEmail"></span>
                                    </a>
                                </p>
                                <p>
                                    <span id="lblHeaderBodyText2">
                                        Thank You for Shopping at Blueocktopus<br>
                                        Scroll down to Experience New Blueocktopus Club <br> privileges specially curated for you.
                                    </span>
                                </p>
                            </div>
                            <div id="divUniVersalTemplateInvoiceBlockInner" class="col-12 invoiceBlockMain">
                                <div class="col-12 invoiceBlock">
                                    <div class="row">
                                        <div id="divamtPaid" class="col-4 invoiceBlockInner" style="display:;">
                                            <div id="divAmountPaid" class="divdropDownData" onclick="fncAmountslide('divTenderDetailRep')" style="display:;">
                                                <label id="lblAmountPaid">Amount Paid</label>
                                                <span id="spnAmountPaidMasterValue">₹&nbsp;@Model.objPOSBILL.GrossAmt</span>
                                            </div>
                                        </div>
                                        <div id="divTotalQuantity" class="col-4 quanity" style="display:;">
                                            <label>Quantity</label>
                                            <span id="spnQuantityMasterValue">@Model.ItemCount</span>
                                        </div>
                                        <div class="col-4 downloadBtn">
                                            <div id="divPdfDownloadPopUp" style="display: none">
                                                <div id="upnlPdFDownload">
                                                    <a id="lnkDownload" class="download" title="Download" href="javascript:WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions(&quot;lnkDownload&quot;, &quot;&quot;, true, &quot;&quot;, &quot;&quot;, false, true))">Download</a>
                                                </div>
                                            </div>
                                            <div id="divPdfDownload" style="display:block;">
                                                <a id="lnkPdfDownload" class="download" title="Download" href="javascript:WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions(&quot;lnkPdfDownload&quot;, &quot;&quot;, true, &quot;&quot;, &quot;&quot;, false, true))">Download</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="divTenderDetailRep" class="totalPointBlock" style="display: none;">
                                <div class="col-12 totalPointBlock" style="display: none;">
                                    <ul>
                                        <li>
                                            <label id="rptTenderDetails_ctl01_lblTenderName">
                                                @Model.objPOSBillMOP.MOPName<span>
                                                    &nbsp;
                                                    <span id="rptTenderDetails_ctl01_lblTenderValue"><span id="rptTenderDetails_ctl01_spnTenderValue">₹&nbsp;</span>@Model.objPOSBILL.GrossAmt</span>
                                                </span>
                                            </label>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                            <div id="divInvoiceDetail" class="invoice_section_wrap portlet" style="display:block;">
