                                    <!-- Edit Highlight Tool -->
                                    <div class="invoice_deatils_wrap">
                                        <div class="invoice_details row">
                                            <div class="col-sm-4 order-det col-6">
                                                <h6 id="h6InvoiceDetailDate">Date</h6>
                                                <p>
                                                    <span id="lblOrderDate">@Model.objPOSBILL.BillOnlyDate</span>
                                                </p>
                                            </div>
                                            <div id="divOrdertime" class="col-sm-4 order-det col-6">
                                                <h6 id="h6InvoiceDetailTime">Order Time</h6>
                                                <p>
                                                    <span id="lblOrdertime">@Model.objPOSBILL.BillOnlyTime</span>
                                                </p>
                                            </div>
                                            <div class="col-sm-4 order-det col-6">
