                                                <p>
                                                    <span id="lblGSTInvoiceNO">@Model.objPOSBILL.GSTDocNumber</span>
                                                </p>
                                            </div>
                                        </div>

                                        <!-- new template change !-->
                                        <div id="divInvoiceUniverseTemplate" class="round_corner_wrap">
                                            <div class="watermark">
                                                <span id="lblUniversalInvoiceDetailwaterMark"></span>
                                            </div>
                                            <img src="./Content/EReceipt/images/round-corner.png" class="round_img" width="100%">
                                            <table class="table table-borderless invoice_table">
                                                <tbody>
                                                    @foreach (var item in Model.lstPOSBillItems)
                                                    {

                                                        <tr>
                                                            <td colspan="4">
                                                                <div class="product_sec_tbl">
                                                                    <div class="img_sec_product">
                                                                        <!-- <img src="images/product.svg" width="100%"> -->
                                                                        <span id="rptUniversalinvoiceDetails_ctl00_lblRowNumber">@count</span>
                                                                    </div>
                                                                    <div style="min-width: 230px;">
                                                                        <!-- <p class="order_code">192283120190</p> -->

                                                                        <b class="product_name">

                                                                            @item.ItemName &nbsp;
                                                                            <div class="tooltip">
                                                                                <img src="./Content/EReceipt/SVG/alert.svg" alt="Alert">
                                                                                <div class="tooltiptext">
                                                                                    <span class="d-flex justify-content-between">
                                                                                        <span>
                                                                                            Code

                                                                                        </span>
                                                                                        <span>@item.BarCode</span>
                                                                                    </span>
                                                                                    <span class="d-flex justify-content-between">
                                                                                        <span id="rptUniversalinvoiceDetails_ctl00_spnHSNHeader">
                                                                                            HSN

                                                                                        </span>
                                                                                        <span id="rptUniversalinvoiceDetails_ctl00_spnHSN">@item.HSNCode</span>
                                                                                    </span>
                                                                                </div>
                                                                            </div>
                                                                        </b>
                                                                        <div class="d-flex align-items-center">
                                                                            <p class="order_qty align-items-center">
                                                                                <span>
                                                                                    <label id="rptUniversalinvoiceDetails_ctl00_lbllabelMRP">MRP:</label>
                                                                                    <span id="rptUniversalinvoiceDetails_ctl00_spnMRP" class="p-0">₹&nbsp;</span>@item.MRP
                                                                                </span>
                                                                                <b>
                                                                                    <label id="rptUniversalinvoiceDetails_ctl00_lbllabelDiscount">Quantity:</label>@item.Qty
                                                                                </b>
                                                                                @*<b>Brand:Bata</b>
                                                    <b>UOM:Prs</b>*@
                                                                            </p>
                                                                            <div id="rptUniversalinvoiceDetails_ctl00_divTaxalert" class="tooltip">
                                                                                <img src="./Content/EReceipt/SVG/alert.svg" alt="Alert">
                                                                                <div class="tooltiptext">
                                                                                    <span class="d-flex justify-content-between">
                                                                                        <span>CGST</span> <span>(@item.CGSTRate %)</span>
                                                                                        <span><span id="rptUniversalinvoiceDetails_ctl00_rptItemQuantityInfoGstSummary_ctl00_spnTaxAmount">₹&nbsp;</span>@item.SGSTAmt</span>
                                                                                    </span>
                                                                                    <span class="d-flex justify-content-between">
                                                                                        <span>SGST</span> <span>(@item.SGSTRate %)</span>
                                                                                        <span><span id="rptUniversalinvoiceDetails_ctl00_rptItemQuantityInfoGstSummary_ctl01_spnTaxAmount">₹&nbsp;</span>@item.SGSTAmt</span>
                                                                                    </span>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div>
                                                                        </div>
                                                                        <p class="order_size">
                                                                            <span>Net Amount</span>
                                                                            <!-- <b>Size:12</b> -->
                                                                        </p>
                                                                    </div>
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <p class="rate_sec">
                                                                    <span>
                                                                        <span id="rptUniversalinvoiceDetails_ctl00_spnSellingPrice">₹&nbsp;</span>
                                                                        @item.NetAmt
                                                                    </span>
                                                                    <span class="flex-column">
                                                                        <span>
                                                                            <span id="rptUniversalinvoiceDetails_ctl00_spnAmount2">₹&nbsp;</span>
                                                                            @item.NetAmt
                                                                        </span>
                                                                    </span>
                                                                </p>
                                                            </td>
                                                        </tr>
                                                        count++;
                                                    }

                                                </tbody>
                                            </table>
                                        </div>
                                        <!-- new template change !-->

                                        <div id="divShowMoreToggle" class="show_more_toggle" style="display: none;">
                                            <div class="gst_summary">
                                                <h4 id="h4TaxSummary" style="padding-top: 0px;">TAX Summary</h4>
                                                <div id="divTaxSummaryAttributes"></div>

                                                <div id="divTaxDetails">
                                                    <table class="table table-borderless">
                                                        <thead>
                                                            <tr id="trTaxDetails">
                                                                <th id="thHSNHeader" scope="col" style="font-weight: 700;">HSN</th>
                                                                <th id="thTAXHeader" scope="col" style="font-weight: 700;">TAX</th>
                                                                <th id="thPercentage" scope="col" style="font-weight: 700;">Rate</th>
                                                                <th id="thTaxableAmountHeader" scope="col" style="font-weight: 700;">Taxable Amt</th>
                                                                <th id="thTaxPaidHeader" scope="col" style="font-weight: 700;">TAX Paid</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var item in Model.lstPOSBillItems)
                                                            {
                                                                <tr id="rptGSTDetails_ctl00_trTaxDetails">
                                                                    <td id="rptGSTDetails_ctl00_tdHSN">
                                                                        @item.HSNCode
                                                                        <br>
                                                                        @item.Article
                                                                    </td>
                                                                    <td>
                                                                        CGST
                                                                    </td>
                                                                    <td id="rptGSTDetails_ctl00_tdtaxPercentage">
                                                                        <span id="rptGSTDetails_ctl00_spntaxPercentage">@item.CGSTRate</span>
                                                                    </td>
                                                                    <td>
                                                                        @item.TaxableAmt
                                                                    </td>
                                                                    <td>
                                                                        @item.CGSTAmt
                                                                    </td>
                                                                </tr>
                                                                <tr id="rptGSTDetails_ctl01_trTaxDetails">
                                                                    <td id="rptGSTDetails_ctl01_tdHSN">
                                                                        @item.HSNCode
                                                                        <br>
                                                                        @item.Article
                                                                    </td>
                                                                    <td>
                                                                        SGST
                                                                    </td>
                                                                    <td id="rptGSTDetails_ctl01_tdtaxPercentage">
                                                                        <span id="rptGSTDetails_ctl01_spntaxPercentage">@item.SGSTRate</span>
                                                                    </td>
                                                                    <td>
                                                                        @item.TaxableAmt
                                                                    </td>
                                                                    <td>
                                                                        @item.SGSTAmt
                                                                    </td>
                                                                </tr>
                                                            }

                                                            <tr>
                                                                <td colspan="12">
                                                                    <div class="devider" style="display: none;"></div>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                                <!--Added for New Template !-->
                                                <div id="divUniversalInvoiceTotalSummary" class="earned_coin_section portlet">
                                                    <ul class="subtotal">
                                                        <li>
                                                            <label id="lblsubtotal">Total RP Value</label>
                                                            <span id="spnSubTotal">₹&nbsp;@Model.TotalTaxableValue</span>
                                                        </li>
                                                        <li>
                                                            <label id="lbltotaltax">Total Tax</label>
                                                            <span id="spnTotalTax">₹&nbsp;@Model.TotalTaxValue</span>
                                                        </li>
                                                        <li>
                                                            <label id="lbltotalamountpaid">Total Amount Paid</label>
                                                            <span id="SpnTotalAmountPaid">₹&nbsp;@Model.objPOSBILL.NetAmt</span>
                                                        </li>
                                                        <li>
                                                            <span id="spnRoundOffAmount"></span>
                                                        </li>
                                                        <li>
                                                            <span id="spnChangeReturned"></span>
                                                        </li>
                                                        <li>
                                                            <label id="rptPaymentAttributes_ctl00_lblPaymentAttributeName">Total MRP Value</label>
                                                            <span id="rptPaymentAttributes_ctl00_spnPaymentAttributeValue">₹&nbsp;@Model.TotalMRPValue</span>
                                                        </li>
                                                    </ul>
                                                </div>
                                                <!--Added for New Template !-->
                                                <div id="divAmountSavedPoint" class="earned_coin_section portlet" style="display:none;">
                                                    <div>
                                                        <!-- Edit Highlight Tool -->
                                                        <div class="editorTool"></div>
                                                        <!-- <i class="oval_wrap_left"></i>
                        <i class="oval_wrap_right"></i>
                        <div class="invoice_edit_icon">
                                    <i class="fa fa-pencil" aria-hidden="true"></i>
                                    <i class="fa fa-arrows" aria-hidden="true"></i>
                                    <i class="fa fa-clone" aria-hidden="true"></i>
                        </div> -->
                                                        <!-- Edit Highlight Tool -->
                                                        <h4 class="coins_para">
                                                            <span id="spnPointsEarned" style="display:block;">
                                                                <img src="./Content/EReceipt/SVG/coins.svg" alt="coins">
                                                                <span id="lblPoitsEarned"> 14</span>
                                                            </span>
                                                            <span id="spnCouponsEarned" style="display: none">
                                                                <img src="./Content/EReceipt/SVG/coins.svg" alt="coins">
                                                                <span id="lblCouponsEarned"></span>
                                                            </span>
                                                        </h4>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- DisplayStoreTerminalDetails section start -->
                                            <div id="divDisplayStoreTerminalDetailsCenter" class="sales_person">
                                                <div id="divTerminalId" class="sales_person_details">
                                                    <h4 id="h6terminalId">Terminal ID</h4>
                                                    <p>
                                                        <span id="lblTerminalIDMasterValue">@Model.objPOSBILL.TerminalId</span>
                                                    </p>
                                                </div>
                                                @*<div id="divPOSNumber" class="sales_person_details">
                        <h4 id="h6PosNumber">POS Number</h4>
                        <p>
                            <span id="lblPOsNumberMasterValue">2499</span>
                        </p>
                    </div>*@
                                                <div id="divSalesAssociateNumber" class="sales_person_details">
                                                    <h4 id="h6SalesAssociate">Sales Associate name</h4>
                                                    <p>
                                                        <span id="lblSaleAssiociateMasterValue">@Model.objPOSBILL.CashierName</span>
                                                    </p>
                                                </div>
                                            </div>
                                            <!-- DisplayStoreTerminalDetails section end -->
                                        </div>

                                        <a onclick="showMoreLess();" id="lbtnShowMore" class="btn button_wrap button_type2" style="font-size:14px;" href="javascript:void(0);"><span id="spnInvoice">Show More</span></a>
                                    </div>
                                    <div id="divFeedBack" class="FeedBackmodal-content feedback_wrap2 mt-3" style="display:block;">
                                        <div class="feedback_wrap2_header">
                                            <!-- modal header  -->
                                            <h4 id="h4Feedback">Please rate your experience</h4>
                                        </div>
                                        <div class="feedback_wrap2_body">
                                            <!-- begin modal body content  -->

                                            <iframe src="F:\BOTS_Code\BOTSA\WebApp\Views\EReceipt\Feedback.html" id="IFrameCollectaURL" width="100%" style="border:none;display:block;height:350px;"></iframe>

                                            <!-- end modal body content  -->
                                        </div>
                                    </div>
                                    <div id="divAvailableAndEarnedOnThisBillPoints" class="Point-Summary" style="display:;">
                                        <div id="h6PointsSummary" class="Point-Summary-header">
                                            Points Summary
                                        </div>
                                        <div class="row Point-Summary-details">
                                            <div class="row text-center fw-500 pt-2 ng-star-inserted col-12" style="height: 10rem;">
                                                <div class="col-6 circle-bg p-4 d-table" style="margin-left:-20px;">
                                                    <div class="align-middle">
                                                        <div style="line-height: 1;">
                                                            <div class="text-green-dark fs-md">Points</div>
                                                            <div class="text-green-dark fs-md">This Bill</div>
                                                        </div>
                                                        <span style="width: 20px; display: inline-block;"></span>
                                                        <div class="text-green-dark">
                                                            <h3 class="m-0 fw-700 text-green-dark">@Model.PointsEarnedWithThisBill</h3>
                                                            <div class="text-green-dark fs-md"></div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-6 circle-bg p-4 d-table" style="margin-left:20px;">
                                                    <div class="align-middle">
                                                        <div class="fs-md" style="line-height: 1;">
                                                            <div class="text-green-dark">Total</div>
                                                            <div class="text-green-dark">Points</div>
                                                        </div>
                                                        <span style="width: 20px; display: inline-block;"></span>
                                                        <div class="text-green-dark ng-star-inserted">
                                                            <h3 class="m-0 fw-700 text-green-dark"> @Model.TotalAvailablePoints </h3>
                                                            <div class="text-green-dark"> </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
