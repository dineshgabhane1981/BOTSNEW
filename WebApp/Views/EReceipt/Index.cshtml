@model BOTS_BL.Models.EReceipt
@{
    Layout = null;
    int count = 1;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <title>receipt</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="./Content/EReceipt/CSS/css2" rel="stylesheet">
    <link href="./Content/EReceipt/CSS/css2(1)" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="./Content/EReceipt/font-awesome.min.css">

    <link rel="stylesheet" type="text/css" href="./Content/EReceipt/CSS/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="./Content/EReceipt/CSS/owl.carousel.css">
    <link id="lnkCSS" rel="stylesheet" type="text/css" href="./Content/EReceipt/CSS/blue.css">
    <link rel="stylesheet" type="text/css" href="./Content/EReceipt/CSS/blue.css">
