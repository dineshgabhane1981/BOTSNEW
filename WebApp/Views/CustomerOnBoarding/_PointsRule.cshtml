    @* jwl fullamt fixed %with box*@
    <div class="row" id="divfullamtfixed" style="display:none;">
        <b>Percentage</b> @Html.DropDownListFor(model => model.objpointsearnruleconfig, new SelectList(Model.Percentage(), "Value", "Text"), "Please Select", htmlAttributes: new { @class = "input50", @id = "ddlFullamtpercentage" })
        @Html.TextBoxFor(model => model.objpointsearnruleconfig.EarnFullAmtPercentageWith, new { @class = "input50", @id = "txtfullAmtWithGstPercentagewith", style = "display: none;" })
        @Html.TextBoxFor(model => model.objpointsearnruleconfig.EarnFullAmtFixedPercentage, new { @class = "input50", @id = "txtfullAmtWithGstPercentage", style = "display: none;" })

    </div>
    @*jwl full amt % with Incremented box*@
    <div class="row">
        <div class="col-md-6 col-sm-6" id="divfullamtpercentagewithincrement" style="display:none;">
            <b>Increment</b>
            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.EarnFullAmtIncremented, "jwlfullamtsingle", new { @id = "rdbfullamtsingle", @class = "fullamtincremented" }) Single
            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.EarnFullAmtIncremented, "jwlfullamtcumulative", new { @id = "rdbfullamtcumulative", @class = "fullamtincremented" }) Cumulative
        </div>

    </div>
    @*jwl full amt incremented single value or cumulative box*@
    <div class="row">
        <div class="col-md-6 col-sm-6" id="divfullamtincrementedpersentage" style="display:none;">
            <b>Percentage</b> @Html.DropDownListFor(model => model.objpointsearnruleconfig, new SelectList(Model.Percentage(), "Value", "Text"), "Please Select", htmlAttributes: new { @class = "input50", @id = "ddlfullamtincrementedpercentage" })
            @Html.TextBoxFor(model => model.objpointsearnruleconfig.EarnFullAmtSingleOrCumulativeWithPercentage, new { @class = "input50", @id = "txtfullamtIncrementpercentagewith", style = "display: none;" })
            @Html.TextBoxFor(model => model.objpointsearnruleconfig.EarnFullAmtSingleOrCumulativeFixedPercentage, new { @class = "input50", @id = "txtfullamtIncrementpercentage", style = "display: none;" })

        </div>
    </div>
    <div class="row">
        @*common 1st fixed percentage box*@
        <div class="col-md-6 col-sm-6" id="divfixed" style="display:none;">
            <b>Percentage</b> @Html.DropDownListFor(model => model.objpointsearnruleconfig.commonfixedpercentageorwithperce, new SelectList(Model.Percentage(), "Value", "Text"), "Please Select", htmlAttributes: new { @class = "input50", @id = "ddlpercentage" })
            @Html.TextBoxFor(model => model.objpointsearnruleconfig.FixedEarnPointPecentageWith, new { @class = "input50", @id = "txtpercentagewith", style = "display: none;" })
            @Html.TextBoxFor(model => model.objpointsearnruleconfig.FixedEarnPointPercentage, new { @class = "input50", @id = "txtpercentage", style = "display: none;" })

        </div>
        @*common slab box*@
        <div class="col-md-6 col-sm-6" id="divslab" style="display:none;">
            <b>Slab Type</b>
            @Html.RadioButtonFor(model => model.objearnpointslab.SlabType, "commonslabin%", new { @id = "rdbFrmToPercentage", @class = "commonslabin%orRs" }) Slab In Percentage
            @Html.RadioButtonFor(model => model.objearnpointslab.SlabType, "commonSlabInRs", new { @id = "rdbFrmToRs", @class = "commonslabin%orRs" }) Slab In RS
            <div class="row" id="commonslabdynamictxt">
                <div id='CommonTextBoxesGroup'>
                    <div id="CommonTextBoxDiv0">
                        <label>From </label><input type='textbox' name="commontextboxfrom" id='commontextboxfrom0' class="input50" placeholder="From">
                        <label>To </label><input type='textbox' name="commontextboxto" id='commontextboxto0' class="input50" placeholder="To">&nbsp;<input type='textbox' name="commontextboxvalue" id='commontextboxvalue0' placeholder="value" style="width:50px;">
                    </div>
                    <input type='button' value='+' id='commonaddButton'>
                    <input type='button' value='-' id='commonremoveButton'>
                </div>
            </div>
        </div>

    </div>
    @*common slab direct or telescope box*@
    <div class="row">
        <div class="col-md-6 col-sm-6" id="divslabDirectTelescoping" style="display:none;">
            <b>Slab Type</b>
            @Html.RadioButtonFor(model => model.objearnpointslab.EarnSlabDirectOrTelescoping, "commonslabdirect", new { @id = "rdbDirect", @class = "commonslabtypeDirect" }) Direct
            @Html.RadioButtonFor(model => model.objearnpointslab.EarnSlabDirectOrTelescoping, "commonslabtelescoping", new { @id = "rdbTelescoping", @class = "commonslabtypeDirect" }) Telescoping
            @Html.TextBoxFor(model => model.objearnpointslab.SlabDirectOrTelescopingValue, new { @class = "input50", @id = "txtcommonslabdirect", style = "display: none;" })
            @Html.TextBoxFor(model => model.objearnpointslab.SlabDirectOrTelescopingValue, new { @class = "input50", @id = "txtcommonslabtelescopic", style = "display: none;" })
        </div>

    </div>
    @*common % with Incremented box*@
    <div class="row">
        <div class="col-md-6 col-sm-6" id="divincrement" style="display:none;">
            <b>Increment</b>
            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.IncrementedValue, "commonincrementedsingle", new { @id = "rdbsingle", @class = "commonsingleorcumulative" }) Single
            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.IncrementedValue, "commonincrementedcumulative", new { @id = "rdbcumulative", @class = "commonsingleorcumulative" }) Cumulative
        </div>

    </div>
    @* common incremented single value or cumulative box*@
    <div class="row">
        <div class="col-md-6 col-sm-6" id="divincrementedpersentage" style="display:none;">
            <b>Percentage</b> @Html.DropDownListFor(model => model.objpointsearnruleconfig.CommonIncrementedPercentageType, new SelectList(Model.Percentage(), "Value", "Text"), "Please Select", htmlAttributes: new { @class = "input50", @id = "ddlincrementedpercentage" })
            @Html.TextBoxFor(model => model.objpointsearnruleconfig.IncrementedpercentageWith, new { @class = "input50", @id = "txtIncrementpercentagewith", style = "display: none;" })
            @Html.TextBoxFor(model => model.objpointsearnruleconfig.IncrementedFixedPercentage, new { @class = "input50", @id = "txtIncrementpercentage", style = "display: none;" })

        </div>
    </div>
    <div class="row">
        <div class="col-md-10 col-sm-10">
            <b>Block For Earn</b> &nbsp;

            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.BlockOnEarnType, "InvoiceAmount", new { @id = "rbgburnforearnpointlevelinvoiceamt", @class = "Burnforearnlevel" }) Invoice Amount
            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.BlockOnEarnType, "Department", new { @id = "rbgburnforearnpointleveldepartment", @class = "Burnforearnlevel" }) Department
            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.BlockOnEarnType, "Product", new { @id = "rbgburnforearnpointlevelproduct", @class = "Burnforearnlevel" }) Product
            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.BlockOnEarnType, "Category", new { @id = "rbgburnforearnpointlevelCategory", @class = "Burnforearnlevel" }) Category
            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.BlockOnEarnType, "SubCategory", new { @id = "rbgburnforearnpointlevelsubcategory", @class = "Burnforearnlevel" }) Sub Category
            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.BlockOnEarnType, "Brand", new { @id = "rbgburnforearnpointlevelbrand", @class = "Burnforearnlevel" }) Brand

        </div>
        <div id="divBurnonEarnUpload" class="col-md-2 col-sm-2" style="padding-right:50px; display:none;">

            <a href="/User/DownloadExcel/"><img src="~/excel.ico" width="25" height="25" title="Download Excel format" alt="excel" /></a>
            <div class="control-label">Excel:</div>
            <input type="file" id="BurnOnEarnUpload" name="BurnOnEarnUpload" class="" />          

        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-sm-12" id="divblockearnInvamtMinMax" style="display:none;">
            <b>Min Amount</b>  @Html.TextBoxFor(model => model.objpointsearnruleconfig.BlockOnInvoiceAmtMin, new { @class = "input50", @id = "txtBlockEarnMin" })
            <b>Max Amount</b> @Html.TextBoxFor(model => model.objpointsearnruleconfig.BlockOnInvoiceAmtMax, new { @class = "input50", @id = "txtBlockEarnMax" })
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-sm-12 pull-right mb-3 mt-3">
            <button type="submit" id="btnRuleSave" class="btn btn-primary pull-right" style="margin-right:10px;" onclick="AddEarnRule();">Save</button>
            <button type="button" id="btnRuleCancel" class="btn btn-primary pull-right" style="margin-right:10px;" onclick="ClearAll();">cancel</button>

        </div>
    </div>
</div>



<script>
            $(document).ready(function () {
                var categoryData = $("#hdnCategoryData").val();
                var CategoryObj = $.parseJSON(categoryData);
                var counter = 2;

               // rdbfullamtslabdirect
                $("input[type='radio'].radiolevel").change(function () {
                    console.log(111);
                    console.log(CategoryObj[0].CategoryId);
                    if (CategoryObj[0].CategoryId == 8) {
                        $("#divjel").show();
                        $("#divnonjel").hide();
                    }
                    else {
                        $("#divjel").hide();
                        $("#divnonjel").show();
                    }
                    if ($("input[type='radio'].radiolevel:checked").val() != "InvoiceAmount") {
                        $("#divEarnUpload").show();
                        
                    }
                    else {
                        $('input[id=rbgburnforearnpointleveldepartment]').attr("disabled", true);
                        $('input[id=rbgburnforearnpointlevelproduct]').attr("disabled", true);
                        $('input[id=rbgburnforearnpointlevelCategory]').attr("disabled", true);
                        $('input[id=rbgburnforearnpointlevelsubcategory]').attr("disabled", true);
                        $('input[id=rbgburnforearnpointlevelbrand]').attr("disabled", true);
                    }

                    

                });
                $("input[type='radio'].Burnforearnlevel").change(function () {
                    //if ($("input[type='radio'].radiolevel:checked").val() == "InvoiceAmount") {

                    //    $('input[id=rbgburnforearnpointleveldepartment]').attr("disabled", true);
                    //    $('input[id=rbgburnforearnpointlevelproduct]').attr("disabled", true);
                    //    $('input[id=rbgburnforearnpointlevelCategory]').attr("disabled", true);
                    //    $('input[id=rbgburnforearnpointlevelsubcategory]').attr("disabled", true);
                    //    $('input[id=rbgburnforearnpointlevelbrand]').attr("disabled", true);
                    //}
                    console.log(222);
                    if ($("input[type='radio'].Burnforearnlevel:checked").val() == "InvoiceAmount") {
                        $("#divblockearnInvamtMinMax").show();
                       
                    }
                    else {
                        $("#divBurnonEarnUpload").show();
                    }
                });
                $("#rdbMaking").change(function () {
                    $("#divmakingjwl").show();//divmakingfixed
                    $("#divfullamt").hide();

                });
                $("#rdbmakingFixed").change(function () {
                    $("#divmakingfixed").show();//rdbmakingSlab
                    $("#divmakingslab").hide();
                });
                $("#rdbmakingSlab").change(function () {
                    $("#divmakingslab").show();//rdbmakingSlab
                    $("#divmakingfixed").hide();
                    $("#divslabdirectortelescoping").show();
                    $("#divfullamt").hide();
                });
                $("#rdbFullAmt").change(function () {
                    $("#divnonjel").hide();
                    $("#divmakingslab").hide();
                    $("#divmakingfixed").hide();
                    $("#divmakingjwl").hide();
                    $("#divslabdirectortelescoping").hide();
                    $("#divfullamt").show();
                    //divmakingjwl
                });//rdbfullamtsingle
                $("#rdbfullamtsingle").change(function () {
                    $("#divfullamtincrementedpersentage").show();

                });
                $("#rdbfullamtcumulative").change(function () {
                    $("#divfullamtincrementedpersentage").show();

                });
                $("#rdbmakingslabdirect").change(function () {
                    $("#txtmakingslabdirect").show();

                });
                $("#rdbmakingslabtelescopic").change(function () {
                    $("#txtmakingslabtelescopic").show();

                });//
                $("#rdbfullamtslabdirect").change(function () {
                    $("#txtfullamtslabdirect").show();
                    $("#txtfullamtslabtelescopic").hide();

                });
                $("#rdbfullamtslabtelescopic").change(function () {
                    $("#txtfullamtslabtelescopic").show();
                    $("#txtfullamtslabdirect").hide();

                });
                $("#rdbDirect").change(function () {
                    $("#txtcommonslabdirect").show();
                    $("#txtcommonslabtelescopic").hide();

                });
                $("#rdbTelescoping").change(function () {
                    $("#txtcommonslabtelescopic").show();
                    $("#txtcommonslabdirect").hide();

                });
                $("#rdbFixed").change(function () {
                    $("#divfixed").show();
                    $("#divslab").hide();
                    $("#divslabDirectTelescoping").hide();
                });
                $("#rdbSlab").change(function () {
                    $("#divfixed").hide();
                    $("#divslab").show();
                    $("#divslabDirectTelescoping").show();
                    $("#divincrement").hide();
                    $("#divincrementedpersentage").hide();

                });
                $("#ddlpercentage").change(function () {
                    console.log(666);
                    if ($("#ddlpercentage").val() == 'percentwith') {
                        $("#txtpercentagewith").show();
                        $("#txtfixedpercentage").hide();
                        $("#divincrement").show();
                    }
                    else if ($("#ddlpercentage").val() == 'fixedpercentage') {
                        $("#txtfixedpercentage").show();
                        $("#divincrement").hide();
                        $("#txtfixedpercentage").hide();
