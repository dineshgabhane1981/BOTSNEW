        @*common fixed box*@
        <div class="col-md-12 col-sm-12" id="divnonjel" style="display:none;">
            <b>Type</b>
            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.EarnPointLevelType, "commonFixed", new { @id = "rdbFixed", @class = "commonfixedorslab" }) Fixed
            @Html.RadioButtonFor(model => model.objpointsearnruleconfig.EarnPointLevelType, "commonSlab", new { @id = "rdbSlab", @class = "commonfixedorslab" }) Slab
        </div>
    </div>
