    <script>
    $(document).ready(function () {
        $("#btnsubmitToDLT").on("click", function () {
            SendDLCToDLT();
        });
        $("#btnDLCLinkSave").on("click", function () {
            SaveDLCLinkConfig();
        });
        LoadExistingDLCLinkData();
        RemoveRequiredBorderDLCLink();
