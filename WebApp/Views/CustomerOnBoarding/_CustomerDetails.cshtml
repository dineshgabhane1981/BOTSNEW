@model WebApp.ViewModel.OnBoardingSalesViewModel
@{
    var retailcount = Model.objRetailList.Count;
    var count = 0;
}
<style>
    .row {
        margin-top: 40px;
        padding: 0 10px;
    }

    .clickable {
        cursor: pointer;
    }

    .panel-heading span {
        margin-top: -20px;
        font-size: 15px;
    }

    .section-heading {
        cursor: pointer;
        text-align: left;
        /*background-color: #022C48;*/
        color: white;
        font-weight: bold;
        padding: 10px;
        border-radius: 5px;
        background-color: #000000 !important;
        background-image: linear-gradient(147deg, #000000 0%, #04619f 74%) !important;
        font-size: 16px;
    }

    .borderClass {
        border: 1px solid red;
    }
    /* The Modal (background) */
    .modal {
        display: none; /* Hidden by default */
        position: fixed; /* Stay in place */
        z-index: 5000 !important; /* Sit on top */
        padding-top: 100px; /* Location of the box */
        left: 120px;
        top: 0;
        width: 100%; /* Full width */
        height: 100%; /* Full height */
        overflow: auto; /* Enable scroll if needed */
        background-color: rgb(0,0,0); /* Fallback color */
        background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
    }

    /* Modal Content */
    .modal-content {
        background-color: #fefefe;
        margin: auto;
        padding: 20px;
        border: 1px solid #888;
        width: 70%;
        border-radius: 25px;
    }

    /* The Close Button */
    .close {
        color: #aaaaaa;
        float: right;
        font-size: 28px;
        font-weight: bold;
    }

        .close:hover,
        .close:focus {
            color: #000;
            text-decoration: none;
            cursor: pointer;
        }

    input:read-only {
        background-color: lightgray;
    }

    .tab1 {
        cursor: pointer;
        padding: 10px 08px;
        margin: 0px 2px;
        background-color: #2f4353 !important;
        display: inline-block;
        color: #fff;
        border-radius: 3px 3px 0px 0px;
        box-shadow: 0 0.5rem 0.8rem #00000080;
        background-color: #000000 !important;
        background-image: linear-gradient(147deg, #000000 0%, #04619f 74%) !important;
    }

    .tabs1 {
        padding-left: 00px;
    }

    .panels1 {
        background: #ffffff;
        box-shadow: 0 1rem 1rem #00000010;
        width: 100%;
        border-radius: 3px;
        overflow: hidden;
    }

    .panel1 {
        display: none;
        animation: fadein .8s;
    }

    .panel-title1 {
        font-size: 1.5em;
        font-weight: bold
    }

    .radio1 {
        display: none;
    }

    #one1:checked ~ .panels1 #one-panel1,
    #two1:checked ~ .panels1 #two-panel1,
    #three1:checked ~ .panels1 #three-panel1,
    #four1:checked ~ .panels1 #four-panel1,
    #five1:checked ~ .panels1 #five-panel1,
    #six1:checked ~ .panels1 #six-panel1,
    #seven1:checked ~ .panels1 #seven-panel1 {
        display: block
    }

    #one1:checked ~ .tabs1 #one-tab1,
    #two1:checked ~ .tabs1 #two-tab1,
    #three1:checked ~ .tabs1 #three-tab1,
    #four1:checked ~ .tabs1 #four-tab1,
    #five1:checked ~ .tabs1 #five-tab1,
    #six1:checked ~ .tabs1 #six-tab1,
    #seven1:checked ~ .tabs1 #seven-tab1 {
        background: #fff !important;
        color: #000;
        border-top: 3px solid #fff;
        border-left: 3px solid #fff;
        border-right: 3px solid #fff;
        font-weight: bold;
        border-top-left-radius: 7px;
        border-top-right-radius: 7px;
    }
</style>
<div class="container-fluid pt-3 pl-3 pr-3 pb-3" id="dvCustomerDetailSection">
    <div class="col-md-12 section-heading" data-toggle="collapse" data-target="#customerdetailsreadonly">Customer Details</div>
    <div id="customerdetailsreadonly" class="collapse">
        <table border="1" cellpadding="4" cellspacing="4" style="margin-top:10px;" width="100%">
            <tr style="background-color:lightgrey;"><td colspan="8" style="font-weight:bold;font-size:18px;">Customer Basic Details</td></tr>
            <tr>
                <td style="font-style: italic; width:110px;">
                    Legal Name
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.GroupName</td>
                <td style="font-style: italic;">
                    Owner’s Name
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.OwnerName</td>
                <td style="font-style: italic; width: 110px;">
                    Owner’s Mobile
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.OwnerMobileNo</td>
                <td style="font-style: italic;">
                    Owner’s Email
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.OwnerEmailId</td>
            </tr>
            <tr>
                <td style="font-style: italic;">
                    GST Document
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.GroupName</td>
                <td style="font-style: italic;">
                    PAN Document
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.OwnerName</td>
                <td style="font-style: italic;">
                    City
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.CityName</td>
                <td style="font-style: italic;">
                    Other Person Name
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.AnyOtherName</td>
            </tr>
            <tr>
                <td style="font-style: italic;">
                    Mobile No
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.AlternateMobileNo</td>
                <td style="font-style: italic;">
                    Email
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.AlternateEmailId</td>
                <td style="font-style: italic;">
                    No of Brands
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.NoOfRetailCategory</td>
                <td style="font-style: italic;">
                    Key Account
                </td>
                @if (Model.bots_TblGroupMaster.IsKeyAccount == 1)
                {
                    <td style="font-weight:bold;">A</td>
                }
                @if (Model.bots_TblGroupMaster.IsKeyAccount == 2)
                {
                    <td style="font-weight:bold;">B</td>
                }
                @if (Model.bots_TblGroupMaster.IsKeyAccount == 3)
                {
                    <td style="font-weight:bold;">C</td>
                }
            </tr>
            <tr>
                <td style="font-style: italic;">
                    DLC
                </td>
                @if (Model.bots_TblGroupMaster.IsMWP.Value)
                {
                    <td style="font-weight:bold;">Yes</td>
                }
                @if (!Model.bots_TblGroupMaster.IsMWP.Value)
                {
                    <td style="font-weight:bold;">No</td>
                }
                <td style="font-style: italic;">
                    Whataspp Integration
                </td>
                @if (Model.bots_TblGroupMaster.IsWhatsApp.Value)
                {
                    <td style="font-weight:bold;">Yes</td>
                }
                @if (!Model.bots_TblGroupMaster.IsWhatsApp.Value)
                {
                    <td style="font-weight:bold;">No</td>
                }
                <td style="font-style: italic;">
                    WA Free Msgs
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.NoOfFreeWhatsAppMsg</td>
                <td style="font-style: italic;">
                    WA Paid Msgs
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.NoOfPaidWhatsAppMsg</td>

            </tr>
            <tr>
                <td style="font-style: italic;">
                    SMS Free Msgs
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.NoOfFreeSMS</td>
                <td style="font-style: italic;">
                    SMS Paid Msgs
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.NoOfPaidSMS</td>
                <td style="font-style: italic;">
                    Mobile App
                </td>
                @if (Model.bots_TblGroupMaster.IsMobileApp == "1")
                {
                    <td style="font-weight:bold;">Yes</td>
                }
                @if (Model.bots_TblGroupMaster.IsMobileApp == "0")
                {
                    <td style="font-weight:bold;">No</td>
                }
                @if (Model.bots_TblGroupMaster.IsMobileApp == null)
                {
                    <td style="font-weight:bold;">No</td>
                }

                <td style="font-style: italic;">
                    Mobile App Description
                </td>

                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.MobileAppDescription</td>
            </tr>

            <tr>
                <td style="font-style: italic;">
                    Existing Loyalty
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.IsExistingLoyalty</td>
                

                
            </tr>


            <tr style="background-color:lightgrey;"><td colspan="8" style="font-weight: bold; font-size: 18px;">Retail Category Details</td></tr>

            @foreach (var item in Model.objRetailList)
            {
                count++;
                <tr>
                    <td style="font-style: italic;">
                        Retail Category
                    </td>
                    <td style="font-weight:bold;">@item.CategoryName</td>
                    <td style="font-style: italic;">
                        Retail / Brand Name
                    </td>
                    <td style="font-weight:bold;">@item.BrandName</td>
                    <td style="font-style: italic;">
                        Total Outlets
                    </td>
                    <td style="font-weight:bold;">@item.NoOfOutlets</td>
                    <td style="font-style: italic;">
                        Enrolled Outlets
                    </td>
                    <td style="font-weight:bold;">@item.NoOfEnrolled</td>
                </tr>
                <tr>
                    <td style="font-style: italic;">
                        BO Product
                    </td>
                    @if (item.BOProduct == "1")
                    {
                        <td style="font-weight:bold;">Octa Plus</td>
                    }
                    @if (item.BOProduct == "2")
                    {
                        <td style="font-weight:bold;">Octa XS</td>
                    }

                    <td style="font-style: italic;">
                        Billing Partner
                    </td>
                    <td style="font-weight:bold;">@item.BillingPartnerName</td>
                    <td style="font-style: italic;">
                        Billing Product
                    </td>
                    <td style="font-weight:bold;">@item.BillingProductName</td>

                    <td colspan="2"></td>

                </tr>
                if (count != retailcount)
                {
                    <tr><td colspan="8" style="font-weight:bold;"></td></tr>
                }
            }

            <tr style="background-color:lightgrey;"><td colspan="8" style="font-weight: bold; font-size: 18px;">Assignment Details</td></tr>
            <tr>
                <td style="font-style: italic;">
                    Sourced By
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.SourceByName</td>
                <td style="font-style: italic;">
                    Assigned CS
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.AssignedCSName</td>
                <td style="font-style: italic;">
                    Source Type
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.SourceTypeName</td>
                <td style="font-style: italic;">
                    Referred Name
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.ReferredName</td>
            </tr>
            <tr>
                <td style="font-style: italic;">
                    Comments
                </td>
                <td style="font-weight:bold;">@Model.bots_TblGroupMaster.Comments</td>
                <td colspan="6"></td>

            </tr>


            <tr style="background-color:lightgrey;"><td colspan="8" style="font-weight: bold; font-size: 18px;">Deal Details</td></tr>
            <tr>
                <td style="font-style: italic;">
                    Loyalty Fees
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.LoyaltyFees</td>
                <td style="font-style: italic;">
                    WA Paid Pack Fees
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.WAPaidPackFees</td>
                <td style="font-style: italic;">
                    SMS Paid Pack Fees
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.SMSPaidPackFees</td>
                <td style="font-style: italic;">
                    Ecomm Integration
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.EcommIntegration</td>
            </tr>
            <tr>
                <td style="font-style: italic;">
                    Any Other Fees
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.AnyOtherFees</td>
                <td style="font-style: italic;">
                    Description
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.AnyOtherFeesDesc</td>
                <td style="font-style: italic;">
                    Total Fees (A)
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.TotalFeesA</td>
                <td style="font-style: italic;">
                    Payment Frequency
                </td>
                @if (Model.bots_TblDealDetails.PaymentFrequency == "1")
                {
                    <td style="font-weight:bold;">Single</td>
                }
                @if (Model.bots_TblDealDetails.PaymentFrequency == "2")
                {
                    <td style="font-weight:bold;">Installments</td>
                }
            </tr>
            <tr>
                <td style="font-style: italic;">
                    GST
                </td>
                @if (Model.bots_TblDealDetails.GST == 1)
                {
                    <td style="font-weight:bold;">CGST/SGST</td>
                }
                @if (Model.bots_TblDealDetails.GST == 2)
                {
                    <td style="font-weight:bold;">IGST</td>
                }
                @if (Model.bots_TblDealDetails.GST == 3)
                {
                    <td style="font-weight:bold;">No</td>
                }
                <td style="font-style: italic;">
                    GST Rate
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.GSTRate</td>
                <td style="font-style: italic;">
                    Total Fees (B)
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.TotalFeesB</td>
                <td style="font-style: italic;">
                    Advance Amount
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.AdvanceAmount</td>
            </tr>
            <tr>
                <td style="font-style: italic;">
                    Payment Mode
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.PaymentMode</td>
                <td style="font-style: italic;">
                    Payment Status
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.PaymentStatus</td>
                <td style="font-style: italic;">
                    Amount Received
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.AmountReceived</td>
                <td style="font-style: italic;">
                    TDS Deducted
                </td>
                <td style="font-weight:bold;">@Model.bots_TblDealDetails.TDSDeducted</td>
            </tr>

            <tr style="background-color:lightgrey;"><td colspan="8" style="font-weight: bold; font-size: 18px;">Payment Details</td></tr>
            <tr>
                <td style="font-style: italic;">
                    Amount Balance
                </td>
                <td style="font-weight:bold;">@Model.bots_TblPaymentDetails.AmountBalance</td>
                <td>
                </td>

                <td colspan="5">
                </td>
            </tr>

            @if (Model.objInstallmentList != null)
            {
                foreach (var item in Model.objInstallmentList)
                {
                    <tr>
                        <td style="font-style: italic;">
                            Installment @item.Installment
                        </td>
                        <td style="font-weight:bold;">@item.PaymentAmount</td>
                        <td style="font-weight:bold;">
                            @item.PaymentDate.ToString("dd/MM/yyyy")
                        </td>
                        <td colspan="5"></td>
                    </tr>
                }
            }
        </table>
    </div>
</div>