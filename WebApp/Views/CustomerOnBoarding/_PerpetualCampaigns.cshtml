    function SaveInactiveConfig(type) {
        var InactiveConfig = [];
        var status = false;
        if (type == "Inactive") {
            if ($("#txtMessageCount").val() == "" || $("#txtMessageCount").val() < 2 || $("#txtMessageCount").val() > 4) {
                cuteAlert({
                    type: "error",
                    title: "Oops..",
                    message: "Message count should be between 2 to 4",
                    buttonText: "Okay"
                })
                $("#txtMessageCount").val("");
                return false;
            }
            else {
                var msgCount = $("#txtMessageCount").val();
                var SMSScriptCount = 1;
                for (var i = 1; i <= msgCount; i++) {
                    item = {}
                    item["Id"] = $('#hdnInactive' + i).val();
                    item["InactiveType"] = "Inactive";
                    item["GroupId"] = $('#hdnGroupID').val();
                    if ($("#rbtnInactiveSMS").prop("checked"))
                        item["SMSorWA"] = "SMS";
                    if ($("#rbtnInactiveWA").prop("checked"))
                        item["SMSorWA"] = "WA";
                    item["Days"] = $('#txtDaysCount' + i).val();
                    item["LessThanDays"] = $('#txtLessThanDays' + i).val();
                    item["LessThanDaysScript"] = $('#txtLessThanDaysScript' + i).val();
                    item["GreaterThanDays"] = $('#txtGreaterThanDays' + i).val();
                    item["GreaterThanDaysScript"] = $('#txtGreaterThanDaysScript' + i).val();
                    item["SMSScript1"] = $('#txtSMSScript2' + SMSScriptCount).val();
                    SMSScriptCount++;
                    item["SMSScript2"] = $('#txtSMSScript2' + SMSScriptCount).val();
                    SMSScriptCount++;
                    InactiveConfig.push(item);
                }
                status = true;
            }
