    function LoadCampaignControls(edata, type) {
        if (edata != null && edata != "") {
            if (type == "Birthday") {
                //console.log(data)
                if (edata.SMSType == "VWA") {
                    $("#rbtnVWA").prop("checked", true);
                    dvBirthDaySMSShowHide(true);
                }
