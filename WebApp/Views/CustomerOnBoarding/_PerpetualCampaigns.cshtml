    function SendPerpetualCampaignToDLT(id, type) {
         $("#divLoader").show();
            $.ajax({
                type: "POST",
                url: '@Url.Action("SendPerpetualCampaignToDLT", "CustomerOnBoarding")',
                data: '{GroupId: ' + JSON.stringify($('#hdnGroupID').val()) + ',CampaignId: ' + JSON.stringify(id) + ',CampaignType: ' + JSON.stringify(type) + '}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (result) {
                    var SuccessMessage = "Campaign sent for DLT Successfully !!!";
                    if (type =="Birthday") {
                        SuccessMessage = "Birthday Configuration sent for DLT Successfully !!!";
                    }
                    if (type == "Anniversary") {
                        SuccessMessage = "Anniversary Campaign sent for DLT Successfully !!!";
                    }
                    if (type == "DLC Referral Reminder") {
                        SuccessMessage = "DLC Referral Reminder Campaign sent for DLT Successfully !!!";
                    }
                    if (type == "DLC Update Reminder") {
                        SuccessMessage = "DLC Update Reminder Campaign sent for DLT Successfully !!!";
                    }
                    if (type == "Balance Updates") {
                        SuccessMessage = "Balance Updates Campaign sent for DLT Successfully !!!";
                    }
                    if (type == "Reminder Bulk Uploaded Users") {
                        SuccessMessage = "Reminder Bulk Uploaded Users Campaign sent for DLT Successfully !!!";
                    }
                    if (type == "Inactive") {
                        SuccessMessage = "Inactive Campaign sent for DLT Successfully !!!";
                    }
                    if (type == "Only Once Inactive") {
                        SuccessMessage = "Only Once Inactive Campaign sent for DLT Successfully !!!";
                    }
                    if (type == "Non Redemption Inactive") {
                        SuccessMessage = "Non Redemption Inactive Campaign sent for DLT Successfully !!!";
                    }
                    if (type == "Point Expiry") {
                        SuccessMessage = "Point Expiry Campaign sent for DLT Successfully !!!";
                    }

                    if (result) {
                        cuteAlert({
                            type: "success",
                            title: "Saved",
                            message: SuccessMessage,
                            buttonText: "Okay"
                        })
                    }
                    else {
                        cuteAlert({
                            type: "error",
                            title: "Oops..",
                            message: "There is a problem while Sending for DLT",
                            buttonText: "Okay"
                        })
                    }
                    $("#divLoader").hide();
                },
                error: function (result) {
                    $("#divLoader").hide();
                }
            });
    }

    function DisableControl(status, control) {
        if (status != null && status != "" && status != "Submitted") {
            $("#" + control).prop("disabled", true);
        }
    }

