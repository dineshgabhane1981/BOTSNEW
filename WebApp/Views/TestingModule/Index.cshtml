    });

    $('#btnSend').click(function () {
        var fromId = "";
        var toId = "";
        var url = "";
        var APIType = "";
        if ($("#rbtnEnrlment").is(":checked")) {
            fromId = "txtRequestPacket1";
            toId = "txtResponsePacket1";
            url = "txtRequestPacketURL1";
