    });

    $("#txtMobileNo2").focusout(function () {
        UpdatePacket2();
    });

    $("#txtMobileNo3").focusout(function () {
        UpdatePacket3();
    });

    $("#txtMobileNo4").focusout(function () {
        UpdatePacket4();
    });

    $("#txtMobileNo5").focusout(function () {
        UpdatePacket5();
    });

    $("#txtMobileNo6").focusout(function () {
        UpdatePacket6();
    });

    $("#txtMobileNo7").focusout(function () {
        UpdatePacket7();
    });
});

