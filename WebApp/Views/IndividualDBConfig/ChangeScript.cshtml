                $("#ddlOutletMaster").val($("#ddlOutletMaster option:first").val());
                $("#ddltblSMSWhatsAppScriptMaster").val($("#ddltblSMSWhatsAppScriptMaster option:first").val());
                $("#txtScript").text($("#txtScript option:first").text());
                $("#ddlWAScriptType").val($("#ddlWAScriptType option:first").val());
            }
            else
                $("#ddlOutletMaster").val("");
                $("#ddltblSMSWhatsAppScriptMaster").val("");
                $("#txtScript").val("");
                $("#ddlWAScriptType").val("");
            $("#dvBirthday").hide();
            $("#dvAnniversary").hide();
            $("#dvInactive").hide();
            $("#dvPointsExpiry").hide();
            $("#dvDLC").hide();
        });
