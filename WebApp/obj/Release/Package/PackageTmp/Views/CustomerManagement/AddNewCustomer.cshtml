                                                        @Html.CheckBoxFor(model => model.objModulesPayment.IsMobileApp, new { @id = "chkIsMobileApp", @style = "margin-right:5px; height: 25px; width: 25px;" }) Mobile App
                                                        <label style="padding-left:15px;"></label>
                                                        @Html.DropDownListFor(model => model.objModulesPayment.MobileAppCharge, Model.OtherDDL(), "Select", htmlAttributes: new { @class = "input50", @disabled = "disabled", @id = "ddlMobileAppCharge" })
                                                        <label style="padding-left:15px;"> Annual Fees charged</label>
                                                        @Html.TextBoxFor(model => model.objModulesPayment.MobileAppAnnualCharge, new { @class = "input50", @id = "txtMobileAppAnnualCharge", @readonly = "readonly" })
                                                    </div>
                                                    <hr />
                                                    <div>
                                                        @Html.CheckBoxFor(model => model.objModulesPayment.IsSProfileUpdate, new { @id = "chkIsSProfileUpdate", @style = "margin-right:5px; height: 25px; width: 25px;" }) Store Profile Update
                                                        <label style="padding-left:15px;"></label>
                                                        @Html.DropDownListFor(model => model.objModulesPayment.SProfileUpdateCharge, Model.OtherDDL(), "Select", htmlAttributes: new { @class = "input50", @disabled = "disabled", @id = "ddlSProfileUpdateCharge" })
                                                        <label style="padding-left:15px;"> Annual Fees charged</label>
                                                        @Html.TextBoxFor(model => model.objModulesPayment.SProfileUpdateAnnualCharge, new { @class = "input50", @id = "txtSProfileUpdateAnnualCharge", @readonly = "readonly" })
                                                    </div>
                                                    <hr />
                                                    <div>
                                                        @Html.CheckBoxFor(model => model.objModulesPayment.IsOtherOne, new { @id = "chkIsOtherOne", @style = "margin-right:5px; height: 25px; width: 25px;" }) Other 1
                                                        <label style="padding-left:15px;"></label>
                                                        @Html.TextBoxFor(model => model.objModulesPayment.OtherOneDesc, new { @class = "input50", @id = "txtOtherOneDesc", placeholder = "Description", @readonly = "readonly" })
                                                        <label style="padding-left:15px;"> Annual Fees charged</label>
                                                        @Html.TextBoxFor(model => model.objModulesPayment.OtherOneFees, new { @class = "input50", @id = "txtOtherOneFees", @readonly = "readonly" })
                                                    </div>
