<script src="~/Content/scriptSteps.js"></script>

<script>
    $(document).ready(function () {
        document.getElementById("FileUploadLogo").addEventListener('change', handleFileSelectInvoice, false);
        $('#ddlRetailCategory').change(function () {
            if ($('#ddlRetailCategory').val() == "14") {
                $('#txtRetailCategory').removeAttr("disabled");
            }
            else {
                $('#txtRetailCategory').val("");
                $('#txtRetailCategory').attr("disabled", "disabled");
            }

        });

        $('#ddlSourcedBy').change(function () {
            if ($('#ddlSourcedBy').val() == "6") {
                $('#txtSourcedBy').removeAttr("disabled");
            }
            else {
                $('#txtSourcedBy').attr("disabled", "disabled");
            }
        });

        $('#ddlBillingPartner').change(function () {
            if ($('#ddlBillingPartner').val() == "10") {
                $('#txtBillingPartner').removeAttr("disabled");
            }
            else {
                $('#txtBillingPartner').attr("disabled", "disabled");
            }
        });

        $('#ddlCity').change(function () {
            if ($('#ddlCity').val() == "11") {
                $('#txtCity').removeAttr("disabled");
            }
            else {
                $('#txtCity').attr("disabled", "disabled");
            }
        });

        $('#ddlRMAssigned').change(function () {
            if ($('#ddlRMAssigned').val() == "3") {
                $('#txtRMAssigned').removeAttr("disabled");
            }
            else {
                $('#txtRMAssigned').attr("disabled", "disabled");
            }
        });

        function handleFileSelectInvoice(evt) {
            var id = evt.currentTarget.id;
            var qId = id.substring(4, id.length);
            var f = evt.target.files[0]; // FileList object
            var reader = new FileReader();
            // Closure to capture the file information.
            reader.onload = (function (theFile) {
                return function (e) {
                    var binaryData = e.target.result;
                    var base64String = window.btoa(binaryData);
                    $("#hdnLogoBase64").val(base64String);
                    $("#hdnLogo").val($("#FileUploadLogo")[0].files[0].name);
                };
            })(f);
            // Read in the image file as a data URL.
            reader.readAsBinaryString(f);
        }

        $('#next1').on("click", function () {
            if ($("#txtRetailName").val() == "" || $("#txtRetailName").val() == null) {
                toastr.error('Please enter Retail Name');
                $("#txtRetailName").focus();
                return false
            }
            if ($("#txtGroupName").val() == "" || $("#txtGroupName").val() == null) {
                toastr.error('Please enter Group Name');
                $("#txtGroupName").focus();
                return false
            }
            if ($("#txtOwnerName").val() == "" || $("#txtOwnerName").val() == null) {
                toastr.error('Please enter Owner Name');
                $("#txtOwnerName").focus();
                return false
            }
            if ($("#txtOwnerMobileNo").val() == "" || $("#txtOwnerMobileNo").val() == null) {
                toastr.error('Please enter Owner Mobile No');
                $("#txtOwnerMobileNo").focus();
                return false
            }
            if ($("#txtOwnerEmail").val() == "" || $("#txtOwnerEmail").val() == null) {
                toastr.error('Please enter Owner Email');
                $("#txtOwnerEmail").focus();
                return false
            }

            $("#hdnOtherRetailCategory").val($("#txtRetailCategory").val());
            $("#hdnOtherSourcedBy").val($("#txtSourcedBy").val());
            $('#hdnOtherBillingPartner').val($("#txtBillingPartner").val());
            $("#hdnOtherCity").val($("#txtCity").val());
