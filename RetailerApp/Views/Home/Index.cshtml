            dateFormat: "yy-mm-dd",
            yearRange: '1950:' + today+'',
            changeYear: true,
            //minDate: new Date(1900, 1 - 1, 1),
            maxDate: '1Y',
        });

        $("#txtADate").datepicker({
            dateFormat: "yy-mm-dd",
            yearRange: '1950:' + today + '',
            changeYear: true,
            //minDate: new Date(1900, 1 - 1, 1),
            maxDate: '1Y',
        });
    });

    function phoneno() {
        $('#txtMobileNo').keypress(function (e) {
            var a = [];
            var k = e.which;

            for (i = 48; i < 58; i++)
                a.push(i);

            if (!(a.indexOf(k) >= 0))
                e.preventDefault();
        });
    }
    function ValidateMobileno() {
        var Temp = $("#txtMobileNo").val();
        const firstDigitStr = String(Temp)[0];
    
        if ($("#txtMobileNo").val() == "") {
            cuteAlert({
                type: "error",
                title: "OOPS...",
                message: "Please enter Mobile number to search",
                buttonText: "Okay"
            })
        }
        else if ((Temp.length) < 10) {
            cuteAlert({
                type: "error",
                title: "OOPS...",
                message: "Please enter Mobile number with 10 digits",
                buttonText: "Okay"
            })
        }
        else if (firstDigitStr < 5) {
            cuteAlert({
                type: "error",
                title: "OOPS...",
                message: "Please enter Mobile number starting greater than 5",
                buttonText: "Okay"
            })
        }
        else {
            SearchCustomer();
        }
    }
